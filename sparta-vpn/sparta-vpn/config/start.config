# Filename : start.config
# Description:
#     This is a simple configuration file read by start.sh

GLOBAL_SETTINGS
        # GRADE_CONTAINER - container where instructor will perform grading
	GRADE_CONTAINER host-11

        # HOST_HOME_XFER - directory to transfer artifact to/from containers
	HOST_HOME_XFER seed_dir/

	# LAB_MASTER_SEED - this is the master seed string specific to this laboratory
	LAB_MASTER_SEED vpnlabseedisnotveryhardtoguess
  REGISTRY oabuoun

# SUBNETS
NETWORK SERVER_LAN
	MASK 10.1.2.0/24
  GATEWAY 10.1.2.253

NETWORK HOST_LAN
	MASK 10.1.1.0/24
	GATEWAY 10.1.1.253

# Container name and settings
CONTAINER server
  TERMINAL_GROUP 2
	USER web-admin
  PASSWORD web-admin
  SERVER_LAN 10.1.2.1
	SCRIPT NONE
	X11 YES


CONTAINER host-11
  TERMINAL_GROUP 1
	USER user-11
  PASSWORD user-11
  HOST_LAN 10.1.1.1
	SCRIPT NONE
	X11 YES
  XTERM INSTRUCTIONS

CONTAINER router
  TERMINAL_GROUP 3
	USER admin
  PASSWORD admin
	SERVER_LAN 10.1.2.254
	HOST_LAN 10.1.1.254
